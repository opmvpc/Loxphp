{
  "name": "opmvpc/loxphp",
  "description": "Lox langage interpreter in php ",
  "keywords": [
    "opmvpc",
    "Loxphp"
  ],
  "homepage": "https://github.com/opmvpc/Loxphp",
  "license": "MIT",
  "authors": [
    {
      "name": "opmvpc",
      "email": "thibsix@outlook.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Opmvpc\\Loxphp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Opmvpc\\Loxphp\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "psalm",
    "test": "phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
